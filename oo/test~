class Rectangle {
  static var width = 10;
  static var height = 12;

  static area() {
    var a = width * height;
    return a;
  }

  static setSize(x, y) {
    width = x;
    height = y;
  }
}

class Square extends Rectangle {
  static setSize(x) {
    var x = 1;
    while (x > 0) {
      super.setSize(x, x);
      x = x - 1;
    }
  }

  static main() {
    setSize(20);
    var x = 2;
    return 2 * area();
  }
}
